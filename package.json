{
	"name": "etp_ms",
	"version": "0.1.0",
	"main": "http://localhost:8080",
	"node-remote": "http://localhost:8080",
	"license": "",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"nw-serve": "nw .",
		"nw-build": "build --tasks linux-x64 --mirror https://dl.nwjs.io/ ."
	},
	"dependencies": {
		"axios": "^0.18.0",
		"chart.js": "^2.9.4",
		"core-js": "2.6.5",
		"echarts": "^5.2.2",
		"js-cookie": "^2.2.0",
		"nw-vue-devtools-prebuilt": "^0.0.10",
		"nwjs-builder-phoenix": "^1.15.0",
		"qs": "^6.9.4",
		"vue": "^2.5.17",
		"vue-chartjs": "^3.5.1",
		"vue-echarts": "^6.0.0",
		"vue-html-to-paper": "^1.3.1",
		"vue-router": "^3.0.1",
		"vue-web-cam": "^1.9.0",
		"vue2-google-maps": "^0.10.7",
		"vuejs-datepicker": "^1.6.2",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.0-rc.12",
		"@vue/cli-plugin-eslint": "^3.0.0-rc.12",
		"@vue/cli-service": "^3.0.0-rc.12",
		"@vue/composition-api": "^1.4.0-beta.0",
		"nw": "^0.53.0-sdk",
		"vue-awesome": "^4.1.0",
		"vue-template-compiler": "^2.5.17"
	},
	"babel": {
		"presets": [
			"@vue/app"
		]
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": "off",
			"no-unused-vars": "off"
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"build": {
		"nwVersion": "0.49.2",
		"strippedProperties": [
			"chromium-args"
		],
		"targets": [
			"zip",
			"nsis7z"
		],
		"excludes": [
			"node_modules/*",
			"src/**/*",
			"src-adm/**/*",
			"src-ins/**/*",
			"src-dep/**/*",
			"src-cas/**/*",
			"public/**/*",
			"dist/*",
			".eslintrc",
			".gitignore",
			"yarn.lock",
			"package-lock.json",
			"vue.config.js"
		]
	},
	"window": {
		"title": "etp_ms",
		"toolbar": false,
		"frame": false,
		"fullscreen": false,
		"width": 1000,
		"height": 660,
		"position": "center",
		"icon": "/public/static/favicon-32.ico",
		"print": {
			"autoprint": true,
			"silent": false,
			"headerFooterEnabled": false,
			"marginsType": 3,
			"marginsCustom": {
				"marginBottom": 16,
				"marginLeft": 16,
				"marginRight": 16,
				"marginTop": 16
			},
			"shouldPrintBackgrounds": true
		}
	},
	"webkit": {
		"plugin": true
	},
	"chromium-args": "--password-store=basic --load-extension='./node_modules/nw-vue-devtools-prebuilt/extension'",
	"manifestUrl": "http://127.0.0.1:3127/package.json"
}
